<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fileblocker.flow.repository.ExtensionRepository">

    <select id="selectCustomExtension" resultType="ExtensionDto">
        SELECT * FROM extension WHERE status=1;
    </select>
    <insert id="insertExtension" parameterType="ExtensionDto">
        INSERT INTO extension VALUES (#{name});
    </insert>

</mapper>